    <None Remove="Scripts\app\item-detail-view.component.ts" />
    <None Remove="Scripts\app\item-list.component.ts" />
    <None Remove="Scripts\app\item.ts" />
    <None Remove="Scripts\app\login.component.ts" />
    <None Remove="Scripts\app\main.ts" />
    <None Remove="Scripts\app\object.ts" />
    <None Remove="Scripts\app\page-not-found.component.ts" />
    <None Remove="Scripts\app\post.service.ts" />
  </ItemGroup>

