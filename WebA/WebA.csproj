  </ItemGroup>

  <ItemGroup>
    <None Remove="Scripts\app\about.component.ts" />
    <None Remove="Scripts\app\app.component.ts" />
    <None Remove="Scripts\app\app.module.ts" />
    <None Remove="Scripts\app\app.routing.ts" />
    <None Remove="Scripts\app\favourites.component.ts" />
